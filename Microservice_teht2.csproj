    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>9b095fe3-41a5-46d7-9a52-0ba1d8a83454</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Data\**" />
